{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app-store": {
      "projectType": "library",
      "root": "app-store",
      "sourceRoot": "app-store/src",
      "prefix": "his",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "app-store/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "app-store/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "app-store/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "app-store/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "app-store/**/*.ts",
              "app-store/**/*.html"
            ]
          }
        }
      }
    },
    "app-store-editor": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": true
        },
        "@schematics/angular:directive": {
          "standalone": true
        },
        "@schematics/angular:pipe": {
          "standalone": true
        }
      },
      "root": "app-store-editor",
      "sourceRoot": "app-store-editor/src",
      "prefix": "his",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/app-store-editor",
            "index": "app-store-editor/src/index.html",
            "main": "app-store-editor/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "app-store-editor/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "app-store-editor/src/favicon.ico",
              "app-store-editor/src/assets"
            ],
            "styles": [
              "app-store-editor/src/styles.scss"
            ],
            "scripts": [],
            "extraWebpackConfig": "app-store-editor/webpack.config.js",
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "app-store-editor/webpack.prod.config.js"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "app-store-editor:build:production",
              "extraWebpackConfig": "app-store-editor/webpack.prod.config.js"
            },
            "development": {
              "browserTarget": "app-store-editor:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4320,
            "publicHost": "http://localhost:4320",
            "extraWebpackConfig": "app-store-editor/webpack.config.js"
          }
        },
        "extract-i18n": {
          "builder": "ngx-build-plus:extract-i18n",
          "options": {
            "browserTarget": "app-store-editor:build",
            "extraWebpackConfig": "app-store-editor/webpack.config.js"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "app-store-editor/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "app-store-editor/src/favicon.ico",
              "app-store-editor/src/assets"
            ],
            "styles": [
              "app-store-editor/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "app-store-editor/**/*.ts",
              "app-store-editor/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "0753126e-ed61-4853-9d14-81650da2a1ea"
  }
}
